{
  "name": "@feelrobotics/keon-wifi-sdk-react",
  "version": "0.3.0",
  "main": "./dist/index.js",
  "module": "./dist/keon-wifi-sdk-react.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "build:cli": "tsc -p tsconfig.cli.json",
    "token": "npm run build:cli && node dist-cli/cli.js"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "OleksiyB",
  "license": "MIT",
  "repository": "https://github.com/FeelRobotics/keon-wifi-sdk",
  "keywords": [
    "Keon",
    "Keon WiFi",
    "Keon WiFi SDK",
    "feelrobotics",
    "kiiroo",
    "bluetooth",
    "ble",
    "wifi",
    "provisioning",
    "react"
  ],
  "size-limit": [
    {
      "path": "dist/keon-wifi-sdk-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/keon-wifi-sdk-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "size-limit": "^11.2.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/web-bluetooth": "^0.0.21",
    "jwt-decode": "^4.0.0",
    "socket.io-client": "^4.8.1"
  }
}
